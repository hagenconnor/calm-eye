{
  "name": "Calm Eye",
  "version": "1.0",
  "description": "Calm Eye",
  "permissions": ["activeTab","declarativeContent","storage"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js","t.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "calmeye.png",
    "32": "calmeye.png",
    "48": "calmeye.png",
    "128": "calmeye.png"
  },
  "manifest_version": 2
}
